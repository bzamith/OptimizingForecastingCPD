[flake8]
ignore = C901, D107, D200, D204, D205, D400, D401, E127, W503, W504, ANN002, ANN003, ANN101, ANN102, ANN204, ANN401
exclude =
    build
    config
    datasets
    __init__.py
    flake8
    env
max-complexity = 10
max-line-length = 200
application-import-names = src, config
import-order-style = cryptography
